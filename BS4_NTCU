import requests
from bs4 import BeautifulSoup
url = 'https://dct.ntcu.edu.tw/news.php'
r = requests.get(url)

if r.status_code == requests.codes.ok:
    
    soup = BeautifulSoup(r.text, 'html.parser')
    
    a_tags = soup.find_all('a')
    for tag in a_tags:
        
        print('ç¶²å€:'+str(tag.string)+' -> '+tag.get('href') )
